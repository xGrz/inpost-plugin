{
    "name": "xgrz/inpost-plugin",
    "description": "InPost plugin for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "inpost"
    ],
    "autoload": {
        "psr-4": {
            "Xgrz\\InPost\\": "src/",
            "Xgrz\\InPost\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xgrz\\InPost\\InPostServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Grzegorz By≈õkiniewicz",
            "email": "grzesiek.byskiniewicz@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
