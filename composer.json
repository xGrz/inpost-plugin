{
  "name": "xgrz/inpost-plugin",
  "description": "InPost plugin for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "inpost"
  ],
  "autoload": {
    "psr-4": {
      "Xgrz\\InPost\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xgrz\\InPost\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Xgrz\\InPost\\InPostServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Grzegorz By≈õkiniewicz",
      "email": "grzesiek.byskiniewicz@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "laravel/prompts": "^0.3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.5",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml.dist --testdox --colors",
    "test-coverage": "phpunit -c phpunit.xml.dist --coverage-text"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
